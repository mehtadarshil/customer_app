{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    "search.exclude": {
        "**/.fvm": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    // Remove from file watching
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "java.configuration.updateBuildConfiguration": "disabled",
    "git.ignoreLimitWarning": true
}